1 VERSIONINFO
 FILEVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_PATCH@,0
 PRODUCTVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_PATCH@,0
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            //VALUE "CompanyName", "TODO: <Company name>"
            VALUE "FileDescription", PACKAGE_NAME_LAST " - @config@ - " @PACKAGE_BUILD_CONFIG@
            VALUE "FileVersion", "@PACKAGE_VERSION_MAJOR@.@PACKAGE_VERSION_MINOR@.@PACKAGE_VERSION_PATCH@"
            //VALUE "InternalName", "@PACKAGE@"
            VALUE "LegalCopyright", "Powered by C++ Archive Network"
            VALUE "OriginalFilename", "@PACKAGE@"
            VALUE "ProductName", "@PACKAGE_NAME@"
            VALUE "ProductVersion", "@PACKAGE_VERSION_MAJOR@.@PACKAGE_VERSION_MINOR@.@PACKAGE_VERSION_PATCH@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
